{
  "name": "thohcm-backend",
  "version": "1.0.0",
  "description": "Thợ HCM - Node.js (Express), MongoDB, Socket.IO - Dịch vụ sửa chữa tại nhà",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest --coverage --runInBand",
    "test:watch": "jest --watch",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "format": "prettier --write \"**/*.{js,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,json,md}\"",
    "seed": "node scripts/seed.js",
    "bootstrap": "node scripts/bootstrap.js",
    "check:atlas": "node scripts/checkAtlas.js",
    "migrate:bookings": "node scripts/migrateBookings.js",
    "audit": "npm audit --audit-level=moderate",
    "audit:fix": "npm audit fix"
  },
  "keywords": ["thohcm", "home-services", "booking", "vietnamese"],
  "author": "ThoHCM Team",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/storage": "^7.17.2",
    "@socket.io/redis-adapter": "^8.3.0",
    "axios": "^1.12.2",
    "bcryptjs": "^3.0.2",
    "cookie-parser": "^1.4.6",
    "compression": "^1.7.4",
    "connect-redis": "^7.1.1",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^8.1.0",
    "express-session": "^1.18.1",
    "express-validator": "^7.2.1",
    "firebase-admin": "^13.5.0",
    "form-data": "^4.0.4",
    "helmet": "^8.1.0",
    "i18next": "^25.6.0",
    "i18next-fs-backend": "^2.6.0",
    "i18next-http-middleware": "^3.8.1",
    "ioredis": "^5.4.1",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "mongoose": "^8.18.1",
    "multer": "^2.0.2",
    "node-fetch": "^2.7.0",
    "qrcode": "^1.5.4",
    "qs": "^6.14.0",
    "rate-limit-redis": "^4.2.0",
    "redis": "^4.7.0",
    "socket.io": "^4.8.1",
    "stripe": "^19.1.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "twilio": "^5.3.7",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.10",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.1.1",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0"
  }
}
